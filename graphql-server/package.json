{
  "name": "graphql-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon --quiet --exec 'node -r dotenv/config server.js'",
    "deploy": "prisma deploy --env-file .env",
    "playground": "prisma playground --env-file .env --port 4002",
    "playground:token": "prisma token --env-file .env"
  },
  "dependencies": {
    "graphql": "^14.0.2",
    "graphql-yoga": "1.16.9",
    "prisma-client-lib": "1.24.0"
  },
  "devDependencies": {
    "dotenv": "6.2.0",
    "nodemon": "1.18.9",
    "prisma": "1.26.6"
  },
  "resolutions": {
    "graphql": "14.0.2"
  }
}
